{
  "defaultSeverity": "error",
  "extends": ["tslint:latest"],
  "jsRules": {},
  "rules": {
    // override tslint-react rules here
    "jsx-wrap-multiline": false,
    "quotemark": [true, "single", "jsx-double", "avoid-escape"],
    "semicolon": [true, "never", "ignore-bound-class-methods"],
    "no-console": false
  },
  "rulesDirectory": []
}
